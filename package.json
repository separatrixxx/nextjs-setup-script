{
  "name": "nextjs-setup-script",
  "version": "1.0.1",
  "description": "A script to set up a Next.js project with additional configurations by separatrix",
  "main": "dist/setup.js",
  "bin": {
    "nextjs-setup-script": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run build",
    "prepack": "npm run build",
    "start": "ts-node src/index.ts"
  },
  "keywords": [
    "nextjs",
    "setup",
    "script"
  ],
  "dependencies": {
    "commander": "^8.0.0",
    "inquirer": "^9.2.23",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/node": "^16.0.0",
    "@types/shelljs": "^0.8.11",
    "ts-node": "^10.0.0",
    "typescript": "^4.0.0"
  },
  "author": "separatrix",
  "license": "MIT"
}
